	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/aicom.git"
	},
	"homepage": "https://github.com/yourusername/aicom#readme",
	"bugs": {
		"url": "https://github.com/yourusername/aicom/issues"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"devDependencies": {
		"@types/node": "^20.0.0",
		"typescript": "^5.0.0"
	},
	"dependencies": {}
